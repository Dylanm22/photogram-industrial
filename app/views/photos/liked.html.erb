<div class="row">
 <div class="col-md-6 offset-md-3">


<h1>Likes</h1>

<ul class="nav nav-pills nav-fill">
  <li class="nav-item">
    <%= link_to "Post", user_path(@user.username), class: "nav-link" %>
  </li>
  <li class="nav-item">
    <%= link_to "Likes", liked_path(@user.username), class: "nav-link active" %>
  </li>
  <li class="nav-item">
    <%= link_to "Feed", feed_path(@user.username), class: "nav-link" %>
</li>
 <li class="nav-item">
    <%= link_to "Following", following_path(@user.username), class: "nav-link" %>
</li>
 <li class="nav-item">
    <%= link_to "Followers", followers_path(@user.username), class: "nav-link" %>
</li>
</ul>



</div>
</div>

<% @user.liked_photos.each do |photo| %>
<%= render "photos/photo", photo: photo %>
<%end %>
